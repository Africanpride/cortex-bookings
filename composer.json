{
    "name": "cortex/bookings",
    "type": "cortex-module",
    "description": "Cortex Bookings is a front layer of a generic resource booking system for Laravel, with the required tools to run your SAAS like services efficiently. It's simple architecture, accompanied by powerful underlying to afford solid platform for your business.",
    "keywords": [
        "rinvex",
        "cortex",
        "flexible",
        "framework",
        "laravel",
        "themes",
        "solution",
        "extension",
        "enterprise",
        "application",
        "multi-lingual",
        "multi-tenant",
        "bootstrap",
        "module"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-bookings/issues",
        "source": "https://github.com/rinvex/cortex-bookings",
        "docs": "https://github.com/rinvex/cortex-bookings/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-bookings/contributors"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "Illuminate/auth": "~5.5.0|~5.6.0",
        "Illuminate/console": "~5.5.0|~5.6.0",
        "Illuminate/http": "~5.5.0|~5.6.0",
        "Illuminate/routing": "~5.5.0|~5.6.0",
        "Illuminate/support": "~5.5.0|~5.6.0",
        "cortex/foundation": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "^4.0.0",
        "laravelcollective/html": "~5.5.0|~5.6.0",
        "league/fractal": "^0.16.0",
        "proengsoft/laravel-jsvalidation": "^2.1.0",
        "rinvex/bookings": "dev-develop",
        "rinvex/composer": "dev-develop",
        "rinvex/fort": "dev-develop",
        "rinvex/menus": "dev-develop",
        "rinvex/support": "dev-develop",
        "spatie/laravel-activitylog": "^2.4.0",
        "spatie/laravel-medialibrary": "v7.x-dev",
        "yajra/laravel-datatables-buttons": "^3.0.0",
        "yajra/laravel-datatables-fractal": "^1.0.0",
        "yajra/laravel-datatables-html": "^3.0.0",
        "yajra/laravel-datatables-oracle": "^8.0.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.4.0",
        "phpunit/phpunit": "^6.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Bookings\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Bookings\\Providers\\BookingsServiceProvider"
            ]
        },
        "cortex-module": {
            "autoinstall": false,
            "icon": {
                "color": "#2196f3",
                "name": "heartbeat",
                "background-size": "cover",
                "background-color": "#e3f2fd",
                "background-position": "center",
                "background-repeat": "no-repeat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
