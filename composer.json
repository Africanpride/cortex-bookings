{
    "name": "cortex/bookings",
    "description": "Cortex Bookings is a front layer of a generic resource booking system for Laravel, with the required tools to run your SAAS like services efficiently. It's simple architecture, accompanied by powerful underlying to afford solid platform for your business.",
    "type": "cortex-module",
    "keywords": [
        "rinvex",
        "cortex",
        "flexible",
        "framework",
        "laravel",
        "themes",
        "solution",
        "extension",
        "enterprise",
        "application",
        "multi-lingual",
        "multi-tenant",
        "bootstrap",
        "module"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-bookings/issues",
        "source": "https://github.com/rinvex/cortex-bookings",
        "docs": "https://github.com/rinvex/cortex-bookings/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-bookings/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "Illuminate/auth": "~5.7.0",
        "Illuminate/console": "~5.7.0",
        "Illuminate/http": "~5.7.0",
        "Illuminate/routing": "~5.7.0",
        "Illuminate/support": "~5.7.0",
        "cortex/auth": "^1.0.0",
        "cortex/foundation": "^1.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.1.0",
        "laravelcollective/html": "~5.7.0",
        "league/fractal": "^0.17.0",
        "proengsoft/laravel-jsvalidation": "^2.3.0",
        "rinvex/laravel-bookings": "^1.0.0",
        "rinvex/laravel-composer": "^1.0.0",
        "rinvex/laravel-menus": "^1.0.0",
        "rinvex/laravel-support": "^1.0.0",
        "silber/bouncer": "v1.0.0-rc.4",
        "spatie/laravel-activitylog": "^2.8.0",
        "spatie/laravel-medialibrary": "^7.5.0",
        "yajra/laravel-datatables-buttons": "^4.2.0",
        "yajra/laravel-datatables-fractal": "^1.2.0",
        "yajra/laravel-datatables-html": "^3.8.0",
        "yajra/laravel-datatables-oracle": "^8.8.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.22.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Bookings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Bookings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Bookings\\Providers\\BookingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
