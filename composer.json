{
    "name": "cortex/bookings",
    "description": "Cortex Bookings is a front layer of a generic resource booking system for Laravel, with the required tools to run your SAAS like services efficiently. It's simple architecture, accompanied by powerful underlying to afford solid platform for your business.",
    "type": "cortex-module",
    "keywords": [
        "rinvex",
        "cortex",
        "flexible",
        "framework",
        "laravel",
        "themes",
        "solution",
        "extension",
        "enterprise",
        "application",
        "multi-lingual",
        "multi-tenant",
        "bootstrap",
        "module"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-bookings/issues",
        "source": "https://github.com/rinvex/cortex-bookings",
        "docs": "https://github.com/rinvex/cortex-bookings/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-bookings/contributors"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "Illuminate/auth": "^6.0.0",
        "Illuminate/console": "^6.0.0",
        "Illuminate/http": "^6.0.0",
        "Illuminate/routing": "^6.0.0",
        "Illuminate/support": "^6.0.0",
        "cortex/auth": "^4.0.0",
        "cortex/foundation": "^3.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.0",
        "laravel/helpers": "^1.1.0",
        "laravelcollective/html": "^6.0.0",
        "league/fractal": "^0.18.0",
        "proengsoft/laravel-jsvalidation": "^2.5.0",
        "rinvex/laravel-bookings": "^3.0.0",
        "rinvex/laravel-composer": "^3.0.0",
        "rinvex/laravel-menus": "^3.0.0",
        "rinvex/laravel-support": "^3.0.0",
        "silber/bouncer": "v1.0.0-rc.6",
        "spatie/laravel-activitylog": "^3.8.0",
        "spatie/laravel-medialibrary": "^7.12.0",
        "yajra/laravel-datatables-buttons": "^4.8.0",
        "yajra/laravel-datatables-fractal": "^1.5.0",
        "yajra/laravel-datatables-html": "^4.16.0",
        "yajra/laravel-datatables-oracle": "^9.6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Bookings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Bookings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Bookings\\Providers\\BookingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
